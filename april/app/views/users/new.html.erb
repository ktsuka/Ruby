<h2>Create New User</h2>
<% if @user.errors.any? %>
  <div id = "error messages">
    <ul>
    <% @user.errors.full_messages.each do |msg| %>
     <li><%= msg %></li>
    <% end %>
    </ul>
  </div>
<% end %>
<%= form_for @user do |form| %>
<%= render "form", form: form %>
<p><%= form.submit %></p>
<% end %>